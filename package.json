{
  "name": "bitonto-crossing",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@10.2.0",
  "engines": {
    "node": ">=20.9.0"
  },
  "scripts": {
    "dev": "node ./node_modules/next/dist/bin/next dev --turbopack",
    "build": "node ./node_modules/next/dist/bin/next build --webpack",
    "start": "node ./node_modules/next/dist/bin/next start",
    "lint": "node ./node_modules/eslint/bin/eslint.js . --max-warnings=0",
    "typecheck": "node ./node_modules/typescript/bin/tsc --noEmit",
    "test": "node ./node_modules/vitest/dist/cli.js run",
    "test:watch": "node ./node_modules/vitest/dist/cli.js",
    "coverage": "node ./node_modules/vitest/dist/cli.js run --coverage",
    "format:check": "node ./node_modules/prettier/bin/prettier.cjs --check .",
    "format:write": "node ./node_modules/prettier/bin/prettier.cjs --write .",
    "quality": "pnpm run format:check && pnpm run lint && pnpm run typecheck && pnpm run coverage"
  },
  "dependencies": {
    "@ducanh2912/next-pwa": "^10.2.9",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "next-themes": "^0.4.6",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^25",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.3",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^10",
    "eslint-config-next": "16.1.6",
    "jsdom": "^28.0.0",
    "prettier": "^3.5.2",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^4.0.18"
  }
}
